<h1>Coding Events</h1>
<p>
    <a asp-controller="Events" asp-action="Add" >Add Item</a>
</p>

@if(ViewBag.events.Count == 0)
{
    <p>No Events yet!</p>
}

<table>
    @foreach (KeyValuePair<string, string> meeting in ViewBag.events)
    {
        <tr>
            <td>@meeting.Key</td>
            <td>@meeting.Value</td>
        </tr>
    }
</table>